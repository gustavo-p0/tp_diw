{
	"extends": ["eslint:recommended", "plugin:import/errors", "prettier"],
	"rules": {
		"react/prop-types": 0,
		"react/react-in-jsx-scope": 0
	},
	"plugins": ["import"],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"env": {
		"es6": true,
		"browser": true
	},
	"settings": {
		"import/resolver": {
			"node": {
				"extensions": [".js"]
			}
		}
	}
}
